<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="1261dp"
  android:layout_height="79dp"
  android:id="@+id/search_flight_item"
  android:background="@drawable/deicing_item_def"
  >
  <ImageView
  	android:layout_width="36dp"
  	android:layout_height="37dp"
  	android:id="@+id/search_flight_item_area"
  	android:padding="2dp"
  	android:layout_alignParentLeft="true"
  />
  <TextView
  	android:layout_width="50dp"
  	android:layout_height="50dp"
  	android:id="@+id/search_flight_item_seqno"
  	android:layout_marginRight="15dp"
  	android:textSize="24dp"
  	android:textColor="#000000"
  	android:layout_gravity="center_vertical"
  	android:layout_marginTop="25dp"
  	android:layout_toRightOf="@id/search_flight_item_area"
  />
  <CheckBox
  	android:layout_width="35dp"
  	android:layout_height="35dp"
  	android:id="@+id/search_flight_item_check"
  	android:layout_marginTop="25dp"
  	android:background="@drawable/check_bg"
  	android:layout_toRightOf="@id/search_flight_item_seqno"
  />
  <TextView
    android:layout_toRightOf="@id/search_flight_item_seqno"
  	android:layout_width="110sp"
  	android:layout_height="wrap_content"
  	android:id="@+id/search_flight_item_plane_num"
  	android:textSize="28dp"
  	android:layout_marginLeft="90dp"
  	android:layout_marginTop="25dp"
  	android:textColor="@color/blue"
  />
  <TextView
  	android:layout_width="120dp"
  	android:layout_height="wrap_content"
  	android:id="@+id/search_flight_item_schedule_num"
  	android:textSize="30dp"
  	android:layout_marginLeft="50dp"
  	android:layout_marginTop="20dp"
  	android:textColor="@color/black"
  	android:layout_toRightOf="@id/search_flight_item_plane_num"
  />
  <TextView
  	android:layout_width="100dp"
  	android:layout_height="wrap_content"
  	android:id="@+id/search_flight_item_conpany"
  	android:textSize="30dp"
  	android:layout_marginLeft="50dp"
  	android:layout_marginTop="20dp"
  	android:textColor="@color/black"
  	android:layout_gravity="center_vertical"
  	android:layout_toRightOf="@id/search_flight_item_schedule_num"
  />
  <TextView
  	android:layout_width="100dp"
  	android:layout_height="wrap_content"
  	android:id="@+id/search_flight_item_model"
  	android:textSize="30dp"
  	android:layout_marginLeft="32dp"
  	android:layout_marginTop="20dp"
  	android:textColor="@color/black"
  	android:layout_toRightOf="@id/search_flight_item_conpany"
  />
  <TextView
  	android:layout_width="150dp"
  	android:layout_height="wrap_content"
  	android:id="@+id/search_flight_item_plan_time"
  	android:textSize="30dp"
  	android:layout_marginLeft="40dp"
  	android:layout_marginTop="20dp"
  	android:textColor="@color/black"
  	android:layout_gravity="center_vertical"
  	android:layout_toRightOf="@id/search_flight_item_model"
  />
  <LinearLayout
  	android:layout_width="wrap_content"
  	android:layout_height="79dp"
  	android:orientation="horizontal"
  	android:layout_marginLeft="32dp"
  	android:layout_toRightOf="@id/search_flight_item_plan_time"
  >
  	<ImageView
  		android:layout_width="50dp"
  		android:layout_height="50dp"
  		android:id="@+id/search_flight_item_state1"
  		android:layout_marginLeft="30dp"
  		android:layout_gravity="center_vertical"
  	/>
  	<ImageView
  		android:layout_width="50dp"
  		android:layout_height="50dp"
  		android:id="@+id/search_flight_item_state2"
  		android:layout_marginLeft="18dp"
  		android:layout_gravity="center_vertical"
  	/>
  	<ImageView
  		android:layout_width="50dp"
  		android:layout_height="50dp"
  		android:id="@+id/search_flight_item_state3"
  		android:layout_marginLeft="17dp"
  		android:layout_gravity="center_vertical"
  	/>
  	<ImageView
  		android:layout_width="50dp"
  		android:layout_height="50dp"
  		android:id="@+id/search_flight_item_state4"
  		android:layout_marginLeft="16dp"
  		android:layout_gravity="center_vertical"
  	/>
  </LinearLayout>
</RelativeLayout>
